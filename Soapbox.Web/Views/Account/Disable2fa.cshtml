@{
    ViewData["Title"] = "Disable two-factor authentication (2FA)";
}

<h2>@ViewData["Title"]</h2>

<div role="alert">
  <p><strong>This action only disables 2FA.</strong></p>
  <p>Disabling 2FA does not change the keys used in authenticator apps. If you wish to change the key used in an authenticator app you should <a asp-page="./ResetAuthenticator">reset your authenticator keys.</a></p>
</div>

<form method="post">
  <button type="submit">Disable 2FA</button>
</form>
