{
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "IdentityOptions": {
    "Password": {
      "RequireDigit": true,
      "RequiredLength": 10,
      "RequireNonAlphanumeric": false,
      "RequireUppercase": true,
      "RequireLowercase": true,
      "RequiredUniqueChars": 6
    },
    "SignIn": {
      "RequireConfirmedAccount": false
    },
    "Lockout": {
      "DefaultLockoutTimeSpan": "00:30:00",
      "MaxFailedAccessAttempts": 10,
      "AllowedForNewUsers": true
    },
    "User": {
      "RequireUniqueEmail": true
    }
  },
  "SqlLite": {
    "ConnectionString": "Filename=./Data/Soapbox.sqlite"
  },
  "SqlServer": {
    "ConnectionString": "Server=(localdb)\\mssqllocaldb;Database=Soapbox;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}